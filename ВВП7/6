#include <iostream>
using namespace std;

int main() {
	int a,b,c;
	cout << "a=";
	cin >> a;
	cout << "b=";
	cin >> b;
	cout << "c=";
	cin >> c;
	if ( c*c==a*a+b*b ) {
		cout <<"Треугольник прямоугольный";
	}
	else if (a*a==b*b+c*c) {
		cout <<"Треугольник прямоугольный";
	}
	else if ( b*b==c*c+a*a) {
		cout <<"Треугольник прямоугольный";
	}
	else { cout << "Треугольник непрямоугольный"; }
	return 0;
}
