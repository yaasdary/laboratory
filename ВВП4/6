#include <iostream>
#include <cmath>
using namespace std;

int main() {
setlocale(LC_ALL, "Russian");
	double a1, b1, c1, a2, b2, c2, x, y, z;
  cout << "Введите коэффициенты 1-го уровнения: ";
  cin >> a1 >> b1 >> c1;   
  cout << "Введите коэффициенты 2-го уравнения: ";
  cin >> a2 >> b2 >> c2;
  
  z=a1*b2-a2*b1;
  x=(c1*b2-c2*b1)/z;
  y=(a1*c2-a2*c1)/z;
  cout << "x= " << x << endl;
  cout << "y= " << y << endl;
	return 0;
}
