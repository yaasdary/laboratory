#include <iostream>
using namespace std;

int main() {
setlocale(LC_ALL, "Russian");
	double N,a;
	cout << "N="; cin >> N;
	a=N/10;
	cout << "0.1 кг = " << a << endl;
	cout << "0.2 кг = " << 2*a << endl;
	cout << "0.3 кг = " << 3*a << endl;
	cout << "0.4 кг = " << 4*a << endl;
	cout << "0.5 кг = " << 5*a << endl;
	cout << "0.6 кг = " << 6*a << endl;
	cout << "0.7 кг = " << 7*a << endl;
	cout << "0.8 кг = " << 8*a << endl;
	cout << "0.9 кг = " << 9*a << endl;
	cout << "1 кг = " << N;
	return 0;
}
